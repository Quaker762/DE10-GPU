{
  "env": {
    "myDefaultIncludePath": [
      "${workspaceFolder}",
      "${workspaceFolder}/emu/"
    ],
  },
  "configurations": [
    {
      "includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/emu/",
        "${workspaceFolder}/emu/rush3d"
      ],
      "cppStandard": "c++14",
      "browse": {}
    }
  ],
  "version": 4,
  "files.associations": {
    "random": "cpp",
    "limits": "cpp"
  },
  "clang-format.executable": "/usr/bin/clang-format",
  "editor.formatOnSave": true
}